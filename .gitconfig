[user]
  name = Sandor Szuecs
  email = sandor.szuecs@fu-berlin.de
[color]
  status = auto
  branch = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[git-tmbundle "log"]
  context-lines = 5
[sendemail]
  smtpencryption = tls
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
  cp = cherry-pick
